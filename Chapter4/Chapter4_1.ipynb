     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "x = np.arange(-5.0, 5.0, 0.1)\n",
    "y = sigmoid(x)\n",
    "\n",
    "plt.plot(x,y,'g')\n",
    "plt.plot([0,0],[1.0,0.0],':')\n",
    "plt.title('Sigmoid Function')\n",
    "plt.show()"
   ]
